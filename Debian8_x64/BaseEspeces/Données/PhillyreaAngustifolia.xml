        <Species>angustifolia</Species>
    </Taxon>
    <DonnéesBotaniques>
        <Feuille>
            <Type>Simple</Type>
            <Forme>Lineaire</Forme>
            <Forme>Lanceolee</Forme>
            <Sommet>Aigue</Sommet>
